#!name=集合定时脚本
#!desc=自用脚本集合
#!arguments=app更新提醒:"", 双色球开奖结果:"", 今日油价:"", 今日汇率:"", 倒数日:"", 今日黄历:"", 彩云天气:"", 彩云天气获参:""
#!arguments-desc=控制开关：默认为空(开启)，在对应项填写 # 即可禁用该脚本。

[Script]
{{{app更新提醒}}}app更新提醒 = type=cron,cronexp=0 8-23/1 * * *,timeout=30,debug=0,script-path=https://raw.githubusercontent.com/loveyuwy/huohua/refs/heads/main/appgx.js,script-update-interval=0

{{{双色球开奖结果}}}双色球开奖结果 = type=cron,cronexp=30 22 * * *,script-path=https://raw.githubusercontent.com/loveyuwy/huohua/refs/heads/main/ssq.js,script-update-interval=0

{{{今日油价}}}今日油价 = type=cron,cronexp=0 9 * * *,script-path=https://raw.githubusercontent.com/loveyuwy/huohua/refs/heads/main/jryj.js,script-update-interval=0,timeout=10,debug=0

{{{今日汇率}}}今日汇率 = type=cron,cronexp=0 9 * * *,script-path=https://raw.githubusercontent.com/Peng-YM/QuanX/master/Tasks/exchange.js,script-update-interval=0,timeout=10

{{{倒数日}}}倒数日 = type=cron,cronexp=30 8 * * *,script-path=https://raw.githubusercontent.com/zqzess/rule_for_quantumultX/master/js/Mine/wnCalendar/DaysMatter.js,timeout=10,script-update-interval=0

{{{今日黄历}}}今日黄历 = type=cron,script-path=https://raw.githubusercontent.com/zqzess/rule_for_quantumultX/master/js/Mine/wnCalendar/wnCalendar.js,timeout=10,cronexp=0 9 * * *,script-update-interval=0

{{{彩云天气}}}彩云天气 = type=cron,cronexp=10 8-22/5 * * *,timeout=30,script-path=https://raw.githubusercontent.com/Peng-YM/QuanX/master/Tasks/caiyun.js,script-update-interval=0

{{{彩云天气获参}}}type = pattern=https:\/\/((weather-data\.apple)|(api.weather))\.com,script-path=https://raw.githubusercontent.com/Peng-YM/QuanX/master/Tasks/caiyun.js,require-body=false

